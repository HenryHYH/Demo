@using FW.Web.Framework.UI;
@{
    Html.AppendResource("~/Content/Style.css");
    Html.AppendResource("~/Scripts/jquery-1.11.2.min.js");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Html.GenerateResources(this.Url, ResourceLocation.Head)
</head>
<body>
    @RenderBody()
    @Html.GenerateResources(this.Url, ResourceLocation.Footer)
</body>
</html>